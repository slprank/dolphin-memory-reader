{
  "name": "dolphin-memory-reader",
  "version": "0.1.0",
  "description": "Node module that can read memory from the Dolphin Emulator",
  "main": "index.node",
  "scripts": {
    "build": "cargo build --message-format=json-render-diagnostics --out-dir . -Z unstable-options",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release && mv dolphin-memory-reader index.node",
    "install": "npm run build-release",
    "test": "cargo test"
  },
  "author": "sindrevatnaland97@gmail.com",
  "url": "https://github.com/slprank/dolphin-memory-reader",
  "repository": {
    "type": "git",
    "url": "https://github.com/slprank/dolphin-memory-reader"
  },
  "license": "MIT",
  "dependencies": {
    "cargo-cp-artifact": "0.1.8"
  },
  "keywords": [
    "memory",
    "process"
  ]
}
